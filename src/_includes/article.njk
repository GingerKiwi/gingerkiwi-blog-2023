---
title: articlename 
layout: 'base.njk'
---
{% include 'header.njk' %}
  <div class="container container-flex">
    <main role="main" class="full-article">
      <article class="article-featured">
        <p>Look for the redesigned</p>
        <h2 class="article-title">{{ title }}</h2>
        <img
            src="{{ image }}"
            alt="{{ imageAlt }}"
            class="article-image article-recent-image full-article-image"
        />
        <p class="article-info">{{ date | postDate }}</p>
        {{ content | safe }}
        
     
      </article>
    </main>
  </div>
  
</html>
